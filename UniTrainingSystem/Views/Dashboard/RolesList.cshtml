@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityRole>
@{
    Layout = null;
}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
@foreach (var role in Model)
{
    <div class="card">
        <div class="card-header">
            Role Number:@role.Id

        </div>
        <div class="card-body">
            <h4>@role.Name</h4>
        </div>
        <div class="card-footer">
            <a asp-action="EditRole" asp-controller="Dashboard" asp-route-id="@role.Id" class="btn btn-secondary">Edit Role</a>

        </div>

    </div>
}
<a asp-action="Index" asp-controller="Dashboard" class="btn btn-danger" style="margin-top:10px;margin-left:20px;">Cancel</a>


