@model TrainingType
@{
    Layout = null;
}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
<form asp-action="AddNewTrainingType" asp-controller="Dashboard" method="post" style="margin-top:50px;">
<div class="container">
    <label asp-for="TypeName"></label>
    <input  asp-for="TypeName" class="form-control"/>

    <input type="submit" value="Add Training Type" class="btn btn-primary" /><br/>
        <a asp-action="Index" asp-controller="Dashboard" style="margin-top:10px; text-decoration:none;" class="btn btn-primary">Back to Dashboard Page</a>

</div>
</form>