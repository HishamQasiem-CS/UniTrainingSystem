@model CreateRoleViewModel
@{
    Layout = null;
}
<h1 style="margin-left:50px;margin-top:20px;">
    Add Admin
</h1>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
<form asp-action="CreateRole" asp-controller="Dashboard" method="Post" style="margin-left:50px;margin-top:20px;">
    <div class="form-control row">
        <label >Admin Name</label>
        <input asp-for="RoleName" class="form-control" placeholder="Enter Role Name" />

    </div>
    <div class="form-control">
        <input type="submit" value="Create Role" class="btn btn-primary" />
        <br/>
        <a asp-action="Index" asp-controller="Dashboard" class="btn btn-danger" style="margin-top:10px;">Cancel</a>

    </div>
</form>
